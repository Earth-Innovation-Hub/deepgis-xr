{
  "options": {
    "paths": {
      "root": "/data",
      "mbtiles": "/data/mbtiles",
      "styles": "/data/styles"
    },
    "domains": ["localhost:8091"],
    "formatQuality": {
      "jpeg": 80,
      "webp": 90
    },
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": true,
    "serveStaticMaps": true
  },
  "styles": {
    "c3": {
      "style": {
        "version": 8,
        "sources": {
          "c3": {
            "type": "raster",
            "url": "mbtiles:///data/mbtiles/C3-29.mbtiles"
          }
        },
        "layers": [
          {
            "id": "c3-layer",
            "type": "raster",
            "source": "c3",
            "minzoom": 0,
            "maxzoom": 22
          }
        ]
      }
    }
  },
  "data": {
    "c3": {
      "mbtiles": "/data/mbtiles/C3-29.mbtiles",
      "type": "raster"
    }
  }
} 